package max;

public class max {
 public static void main(String args[]){
   int a[][]={{1,4,3},{4,5,6},{7,8,9}};
   for(int i=0;i<3;i++){
    int max=Integer.MIN_VALUE;
    for(int j=0;j<3;j++){
       if(a[i][j]>max)
        max=a[i][j];
     }
   System.out.println(max);{}
   }
   
  }
  }
//12jan
